{
  "name": "khafidprayoga/php-microsite",
  "type": "project",
  "require": {
    "bramus/router": "~1.6",
    "monolog/monolog": "^3.8",
    "php-di/php-di": "^7.0",
    "doctrine/orm": "3",
    "doctrine/dbal": "4",
    "symfony/cache": "7",
    "twig/twig": "3.18.0",
    "symfony/http-foundation": "^7.2@beta",
    "friendsofphp/proxy-manager-lts": "^1.0",
    "symfony/serializer-pack": "^1.3"
  },
  "autoload": {
    "psr-4": {
      "Khafidprayoga\\PhpMicrosite\\": "src/"
    }
  },
  "authors": [
    {
      "name": "Khafid Prayoga",
      "email": "khafidp@pm.me"
    }
  ],
  "minimum-stability": "beta",
  "scripts": {
    "dev": [
      "Composer\\Config::disableProcessTimeout",
      "php -S localhost:8000 index.php"
    ],
    "lint": "./vendor/bin/php-cs-fixer fix src",
    "dev-apply-migration": "php bin/doctrine orm:schema-tool:update --force --complete"
  },
  "config": {
    "platform": {
      "php": "8.3.14"
    }
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^3.68"
  }
}
